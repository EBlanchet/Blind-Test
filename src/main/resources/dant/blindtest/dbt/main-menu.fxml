<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<AnchorPane id="menu" xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="dant.blindtest.dbt.MainMenuController"
            prefHeight="400.0" prefWidth="600.0">
    <VBox alignment="CENTER" spacing="20.0" prefWidth="600.0" prefHeight="400.0">

        <Label fx:id="title">DANT BlindTest v0.1</Label>

        <ImageView id="Logo">
        </ImageView>

        <Button fx:id="newGameButton" text="Nouvelle Partie" onAction="#newGameButtonAction"/>
        <Button fx:id="exitButton" text="Quitter" onAction="#exitButtonAction"/>
    </VBox>
</AnchorPane>
